{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "target": "es2016",
        "module": "commonjs",
        "moduleDetection": "force",
        "outDir": "./built",
        "skipLibCheck": true,
        "strict": true
    },
    "include": [
        "./"
    ],
    "exclude": [
        "./node_modules"
    ]
}